#=========================================================
# OpenShift template parameters for:
# Component: .
# Template File: ../openshift/templates/event-processor-log-db/event-processor-log-db-deploy.json
#=========================================================
NAME=event-processor-log-db
SUFFIX=-indy-cat
APP_NAME=BcRegistriesAgent
APP_GROUP=aries-vcr
IMAGE_NAMESPACE=devex-von-bc-registries-agent-tools
SOURCE_IMAGE_NAME=db
TAG_NAME=dev
POSTGRESQL_DATABASE_NAME=mara_db
# POSTGRESQL_USER=[a-zA-Z_][a-zA-Z0-9_]{10}
# POSTGRESQL_PASSWORD=[a-zA-Z0-9_~!@#$%^&*()-=<>,.?;:|]{16}
# POSTGRESQL_ADMIN_PASSWORD=[a-zA-Z0-9_~!@#$%^&*()-=<>,.?;:|]{16}
MOUNT_PATH=/var/lib/pgsql/data
PERSISTENT_VOLUME_SIZE=5Gi
PERSISTENT_VOLUME_CLASS=gluster-file-db
CPU_REQUEST=10m
CPU_LIMIT=250m
MEMORY_REQUEST=10Mi
MEMORY_LIMIT=512Mi
